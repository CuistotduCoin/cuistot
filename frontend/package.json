{
  "name": "cuistot-front",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:CuistotduCoin/cuistot.git"
  },
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy": "yarn build && serverless client deploy",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "dependencies": {
    "@jaredpalmer/after": "^1.3.1",
    "@material-ui/icons": "^1.0.0-beta.43",
    "apollo-client": "^2.3.1",
    "apollo-client-preset": "^1.0.8",
    "aws-amplify": "^0.3.3",
    "aws-amplify-react": "^0.1.43",
    "aws-appsync": "^1.0.19",
    "aws-appsync-react": "^1.0.10",
    "aws-sdk": "^2.238.1",
    "express": "^4.16.3",
    "formik": "^0.11.11",
    "graphql": "^0.13.2",
    "graphql-anywhere": "^4.1.10",
    "graphql-tag": "^2.9.2",
    "material-ui": "^1.0.0-beta.47",
    "node": "10.0.0",
    "razzle": "^2.0.0-alpha.12",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "typeface-roboto": "^0.0.54"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.1",
    "@storybook/addon-info": "^4.0.0-alpha.1",
    "@storybook/addon-knobs": "^4.0.0-alpha.1",
    "@storybook/addon-options": "^4.0.0-alpha.1",
    "@storybook/react": "^4.0.0-alpha.1",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.8",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "@types/storybook__addon-knobs": "^3.3.1",
    "@types/storybook__addon-options": "^3.2.0",
    "@types/storybook__react": "^3.0.7",
    "@types/webpack-env": "^1.13.6",
    "prettier": "^1.12.1",
    "raf": "^3.4.0",
    "serverless-finch": "^2.1.2",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "tsconfig-paths-webpack-plugin": "^3.0.4",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-loader": "wbuchwalter/tslint-loader",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
