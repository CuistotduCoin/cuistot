{
  "name": "cuistot-front",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:CuistotduCoin/cuistot.git"
  },
  "scripts": {
    "start": "env-cmd .env cross-env EXECUTION_ENV=start razzle start",
    "debug": "env-cmd .env cross-env EXECUTION_ENV=start razzle start --inspect",
    "debugStart": "env-cmd .env cross-env EXECUTION_ENV=start razzle start --inspect-brk",
    "build": "env-cmd .env cross-env EXECUTION_ENV=lambda razzle build",
    "startLambda": "env-cmd .env cross-env EXECUTION_ENV=lambda serverless offline start",
    "test": "env-cmd .env razzle test --env=jsdom",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy": "env-cmd .env cross-env EXECUTION_ENV=lambda serverless deploy",
    "tslint-check": "env-cmd .env tslint-config-prettier-check ./tslint.json",
    "build:prod": "cross-env NODE_ENV=production assume-role production node loadenv && env-cmd .env cross-env CI=false RAZZLE_PUBLIC_DIR=public razzle build",
    "deploy:prod": "assume-role production serverless deploy -s production"
  },
  "dependencies": {
    "@jaredpalmer/after": "^1.3.1",
    "@material-ui/core": "^3.0.3",
    "@material-ui/docs": "^1.0.0-alpha.5",
    "@material-ui/icons": "^3.0.1",
    "algolia-places-react": "^1.3.0",
    "algoliasearch": "^3.30.0",
    "autotrack": "^2.4.1",
    "aws-sdk": "^2.327.0",
    "aws-serverless-express": "^3.3.5",
    "express": "^4.16.3",
    "formik": "^1.2.0",
    "formik-material-ui": "^0.0.9",
    "raven": "^2.6.4",
    "react": "^16.5.0",
    "react-credit-cards": "^0.7.0",
    "react-dom": "^16.5.0",
    "react-helmet": "^5.2.0",
    "react-hubspot-form": "^1.3.5",
    "react-instantsearch-dom": "^5.2.3",
    "react-instantsearch-dom-maps": "^5.2.3",
    "react-intl": "^2.4.0",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.10",
    "react-slick": "^0.23.1",
    "react-waypoint": "^8.0.3",
    "serialize-javascript": "^1.5.0",
    "serverless-sentry-lib": "^1.1.2",
    "slick-carousel": "^1.8.1",
    "typeface-roboto": "^0.0.54",
    "yup": "^0.26.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.21",
    "@storybook/addon-info": "^4.0.0-alpha.21",
    "@storybook/addon-knobs": "^4.0.0-alpha.21",
    "@storybook/addon-options": "^4.0.0-alpha.21",
    "@storybook/react": "^4.0.0-alpha.21",
    "@types/aws-serverless-express": "^3.0.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.7",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-scroll": "^1.5.4",
    "@types/react-slick": "^0.23.2",
    "@types/react-sticky": "^6.0.2",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__addon-options": "^3.2.2",
    "@types/storybook__react": "^3.0.9",
    "@types/webpack-env": "^1.13.6",
    "@types/yup": "^0.24.9",
    "aws-param-env": "^1.2.0",
    "cross-env": "^5.2.0",
    "env-cmd": "^8.0.2",
    "node": "^10.11.0",
    "prettier": "^1.14.2",
    "razzle": "^2.4.0",
    "razzle-plugin-typescript": "^2.4.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-domain-manager": "^2.6.5",
    "serverless-dotenv-plugin": "^1.2.0",
    "serverless-offline": "^3.25.11",
    "serverless-sentry": "^1.2.0",
    "serverless-stage-manager": "^1.0.5",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}