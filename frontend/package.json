{
  "name": "cuistot-front",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:CuistotduCoin/cuistot.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development && assume-role development node loadenv && env-cmd .env nodemon server/index.ts",
    "dev-win": "cross-env NODE_ENV=development && env-cmd .env nodemon server/index.ts",
    "build:dev": "NODE_ENV=development assume-role development node loadenv && env-cmd .env next build && tsc --project tsconfig.server.json && webpack",
    "build-win": "cross-env NODE_ENV=production env-cmd .env next build && tsc --project tsconfig.server.json && webpack",
    "build:stage": "cross-env NODE_ENV=staging assume-role staging node loadenv && env-cmd .env next build && tsc --project tsconfig.server.json && webpack",
    "build:prod": "cross-env NODE_ENV=production assume-role production node loadenv && env-cmd .env next build && tsc --project tsconfig.server.json && webpack",
    "webpack": "webpack",
    "lambda:dev": "assume-role development serverless offline start -s development",
    "lambda:prod": "assume-role production serverless offline start -s production",
    "start": "cross-env NODE_ENV=development assume-role development node build/index.js",
    "start-win": "cross-env NODE_ENV=development node build/index.js",
    "deploy:dev": "assume-role development serverless deploy -s development",
    "deploy-win": "serverless deploy -s development",
    "deploy:stage": "assume-role staging serverless deploy -s staging",
    "deploy:prod": "assume-role production serverless deploy -s production",
    "storybook": "start-storybook -p 9001",
    "tslint-check": "cross-env NODE_ENV=development assume-role development node loadenv && env-cmd .env tslint-config-prettier-check ./tslint.json"
  },
