{
  "name": "cuistot-front",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:CuistotduCoin/cuistot.git"
  },
  "scripts": {
    "start": "env-cmd .env razzle start",
    "startLambda": "env-cmd .env serverless offline start",
    "build": "env-cmd .env razzle build",
    "test": "env-cmd .env razzle test --env=jsdom",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy": "env-cmd .env serverless deploy",
    "tslint-check": "env-cmd .env tslint-config-prettier-check ./tslint.json"
  },
  "dependencies": {
    "@jaredpalmer/after": "^1.3.1",
    "@material-ui/core": "^1.2.3",
    "@material-ui/docs": "^1.0.0-alpha.3",
    "@material-ui/icons": "^1.1.0",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link-http": "^1.5.4",
    "aws-amplify": "^0.4.6",
    "aws-amplify-react": "^0.1.52",
    "aws-appsync": "^1.1.2",
    "aws-appsync-react": "^1.0.17",
    "aws-sdk": "^2.263.1",
    "aws-serverless-express": "^3.2.0",
    "express": "^4.16.3",
    "formik": "^1.0.0-beta.2",
    "formik-material-ui": "^0.0.5",
    "graphql": "^0.13.2",
    "graphql-anywhere": "^4.1.14",
    "graphql-tag": "^2.9.2",
    "node": "^10.5.0",
    "razzle": "^2.2.0",
    "razzle-plugin-typescript": "^2.2.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.6",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.23.1",
    "slick-carousel": "^1.8.1",
    "typeface-roboto": "^0.0.54",
    "yup": "^0.25.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.8",
    "@storybook/addon-info": "^3.4.6",
    "@storybook/addon-knobs": "^4.0.0-alpha.8",
    "@storybook/addon-options": "^4.0.0-alpha.8",
    "@storybook/react": "^4.0.0-alpha.8",
    "@types/amazon-cognito-auth-js": "^1.2.0",
    "@types/aws-serverless-express": "^3.0.1",
    "@types/express": "^4.16.0",
    "@types/graphql": "^0.13.1",
    "@types/jest": "^23.1.2",
    "@types/node": "10.1.4",
    "@types/prop-types": "^15.5.3",
    "@types/react": "16.3.14",
    "@types/react-dom": "^16.0.6",
    "@types/react-helmet": "^5.0.6",
    "@types/react-router": "^4.0.27",
    "@types/react-router-dom": "4.2.6",
    "@types/react-slick": "^0.23.1",
    "@types/storybook__addon-knobs": "^3.3.1",
    "@types/storybook__addon-options": "^3.2.1",
    "@types/storybook__react": "^3.0.7",
    "@types/webpack-env": "^1.13.6",
    "@types/yup": "^0.24.7",
    "env-cmd": "^8.0.2",
    "prettier": "^1.13.6",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-domain-manager": "^2.4.0",
    "serverless-dotenv-plugin": "^1.1.3",
    "serverless-offline": "^3.25.4",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.4.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "tslint-loader": "^3.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "2.8.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
