  "scripts": {
    "dev": "NODE_ENV=development assume-role development node loadenv && env-cmd .env nodemon server/index.ts",
    "build:dev": "NODE_ENV=development assume-role development node loadenv && env-cmd .env next build && tsc --project tsconfig.server.json",
    "build:stage": "NODE_ENV=staging assume-role staging node loadenv && env-cmd .env next build && tsc --project tsconfig.server.json",
    "build:prod": "NODE_ENV=production assume-role production node loadenv && env-cmd .env next build && tsc --project tsconfig.server.json",
    "lambda:dev": "assume-role development serverless offline start -s development",
    "lambda:prod": "assume-role production serverless offline start -s production",
    "start": "NODE_ENV=development assume-role development node build/index.js",
    "deploy:dev": "assume-role development serverless deploy -s development",
    "deploy:stage": "assume-role staging serverless deploy -s staging",
    "deploy:prod": "assume-role production serverless deploy -s production",
    "storybook": "start-storybook -p 9001",
    "tslint-check": "NODE_ENV=development assume-role development node loadenv && env-cmd .env tslint-config-prettier-check ./tslint.json"
  },
