{
  "name": "cuistot-back",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:CuistotduCoin/cuistot.git"
  },
  "scripts": {
    "dynamodb-generate-data": "cd seed-data && node create_seed_data.js",
    "dynamodb-insert-seed-data-prod": "cd seed-data && node insert_seed_data_prod.js",
    "start": "yarn dynamodb-generate-seed-data && serverless dynamodb install && serverless offline start",
    "deploy": "yarn dynamodb-generate-seed-data && serverless deploy && yarn dynamodb-insert-seed-data-prod",
    "deploy-appsync": "serverless deploy-appsync",
    "update-appsync": "serverless update-appsync",
    "delete-appsync": "serverless delete-appsync"
  },
  "dependencies": {
    "aws-sdk": "^2.256.1",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "node-fetch": "^2.1.2",
    "serverless-appsync-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "cors": "2.8.4",
    "env-cmd": "^8.0.2",
    "faker": "^4.1.0",
    "imports-loader": "^0.8.0",
    "jest": "^23.2.1",
    "jsonfile": "^4.0.0",
    "serverless-dynamodb-local": "^0.2.30",
    "serverless-offline": "^3.25.4",
    "serverless-webpack": "^5.1.5",
    "webpack": "^4.12.0",
    "webpack-node-externals": "^1.7.2"
  }
}