{
  "name": "cuistot-back",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:CuistotduCoin/cuistot.git"
  },
  "scripts": {
    "migrate-prod": "env-cmd .env.prod node_modules/.bin/knex migrate:latest",
    "rollback-prod": "env-cmd .env.prod node_modules/.bin/knex migrate:rollback",
    "seed-prod": "env-cmd .env.prod node_modules/.bin/knex seed:run",
    "start-prod": "env-cmd .env.prod serverless offline start -s production",
    "deploy-prod": "env-cmd .env.prod serverless deploy --force -s production",
    "setup-db-dev": "env-cmd .env.dev ./scripts/setup.sh",
    "migrate-dev": "env-cmd .env.dev node_modules/.bin/knex migrate:latest",
    "rollback-dev": "env-cmd .env.dev node_modules/.bin/knex migrate:rollback",
    "seed-dev": "env-cmd .env.dev node_modules/.bin/knex seed:run",
    "start-dev": "env-cmd .env.dev serverless offline start -s development",
    "deploy-dev": "env-cmv .env.dev serverless deploy --force -s development"
  },
  "dependencies": {
    "async": "^2.6.1",
    "aws-sdk": "^2.298.0",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "dotenv": "^6.0.0",
    "gm": "^1.23.1",
    "knex": "^0.15.1",
    "mjml": "^4.1.2",
    "mjml-utils": "^2.2.1",
    "node-fetch": "^2.1.2",
    "nodemailer": "^4.6.8",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "copy-webpack-plugin": "^4.5.2",
    "cors": "2.8.4",
    "env-cmd": "^8.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "faker": "^4.1.0",
    "imports-loader": "^0.8.0",
    "jest": "^23.1.0",
    "jsonfile": "^4.0.0",
    "serverless-appsync-plugin": "^1.0.1",
    "serverless-dotenv-plugin": "^1.1.3",
    "serverless-offline": "^3.25.6",
    "serverless-stage-manager": "^1.0.5",
    "serverless-webpack": "^5.2.0",
    "uuid": "^3.3.2",
    "webpack": "^4.12.0",
    "webpack-node-externals": "^1.7.2"
  }
